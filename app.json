    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": "./assets/fonts/JetBrains-Mono-Regular.ttf'",
    "extra": {
      "eas": {
        "projectId": "97abc5f1-8ed2-4137-92fe-623aa2fd46c9"
